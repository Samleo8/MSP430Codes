/***************************
 * MAIN.H
 * Use this header file to attach functions and define constants
****************************/

#include "msp430fr4133.h"

//Modes
#define MODE_STARTUP 0

#define MODE_TYPING 1
#define MODE_TYPE MODE_TYPING
#define MODE_NUMBER MODE_TYPING

#define MODE_ADD 2

#define MODE_SUBTRACT 3
#define MODE_MINUS MODE_SUBTRACT

//Number parsing
#define TRUE 0xFF
#define FALSE 0x00

#define ABS(n) (n>0)?n:-n
#define MAX(m,n) (m>n)?m:n
#define MIN(m,n) (m<n)?m:n


void Init_GPIO(void);
void Init_Clock(void);
void Init_ADC(void);
